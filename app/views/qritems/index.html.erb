<h1>Listing qritems</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @qritems.each do |qritem| %>
  <tr>
    <td><%= qritem.tag %></td>
    <td><%= qritem.url %></td>
    <td><%= link_to 'Show', qritem %></td>
    <td><%= link_to 'Edit', edit_qritem_path(qritem) %></td>
    <td><%= link_to 'Destroy', qritem, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Qritem', new_qritem_path %>
